{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.0",
    "generators": {
      "csharp": {
        "inputSpec": "swagger.json",
        "output": "./src",
        "generatorName": "typescript",
        "globalProperty": "apiTests=false,apiDocs=false,modelTests=false,modelDocs=false",
        "additionalProperties": {
          "disallowAdditionalPropertiesIfNotPresent": true,
          "framework": "fetch-api",
          "legacyDiscriminatorBehavior": false,
          "npmName": "denji-public-client",
          "npmVersion": "1.0.0-alpha01",
          "platform": "browser"
        }
      }
    }
  }
}